library verilog;
use verilog.vl_types.all;
entity counter is
    generic(
        w               : integer := 4
    );
    port(
        clk             : in     vl_logic;
        clear           : in     vl_logic;
        enable          : in     vl_logic;
        q               : out    vl_logic_vector
    );
end counter;
